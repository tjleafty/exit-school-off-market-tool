# Supabase Edge Functions Environment Variables
# Copy this file to .env and add your actual API keys

# Supabase Configuration (required)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# OpenAI API for report generation (required)
OPENAI_API_KEY=your_openai_api_key_here

# Email service (required for email campaigns)
RESEND_API_KEY=your_resend_api_key_here

# Data enrichment services (optional but recommended)
HUNTER_API_KEY=your_hunter_io_api_key_here
APOLLO_API_KEY=your_apollo_api_key_here
ZOOMINFO_API_KEY=your_zoominfo_api_key_here

# PDF generation service (optional)
PUPPETEER_API_KEY=your_puppeteer_service_api_key_here

# Webhook URLs for notifications (optional)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here